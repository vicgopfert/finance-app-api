generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id         String   @id @default(uuid())
  first_name String   @db.VarChar(50)
  last_name  String   @db.VarChar(50)
  email      String   @db.VarChar(100) @unique
  password   String   @db.VarChar(100)
}